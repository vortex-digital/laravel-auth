{
    "name": "vortex-digital/laravel-simple-auth-installer",
    "description": "Simple authentication like it was before Jetstream and Fortify, and ready to go ðŸ”¥",
    "version": "1.0.0",
    "keywords": [
        "vortex-digital",
        "laravel-simple-auth-installer"
    ],
    "homepage": "https://github.com/vortex-digital/laravel-auth",
    "license": "MIT",
    "authors": [
        {
            "name": "sergeByishimo",
            "email": "sergebyishimo@gmail.com",
            "homepage": "https://serge.works",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": "^7.3",
        "symfony/console": "^4.0|^5.0",
        "symfony/process": "^4.2|^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "bin": [
        "bin/laravel-simple"
    ],
    "autoload": {
        "psr-4": {
            "LaravelSimple\\Installer\\Console\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelSimple\\Installer\\Console\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\Skeleton\\SkeletonServiceProvider"
            ],
            "aliases": {
                "Skeleton": "Spatie\\Skeleton\\SkeletonFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
